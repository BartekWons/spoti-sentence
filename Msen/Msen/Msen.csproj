<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Images\close_icon.png" />
    <None Remove="Assets\Images\minimize_icon.png" />
    <None Remove="Assets\Sounds\click_sound.wav" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Images\close_icon.png" />
    <Resource Include="Assets\Images\minimize_icon.png" />
    <Resource Include="Assets\Sounds\click_sound.wav" />
  </ItemGroup>

</Project>
